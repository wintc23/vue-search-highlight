{
  "name": "vue-search-highlight",
  "version": "1.1.7",
  "keywords": [
    "搜索高亮",
    "vue",
    "关键字高亮"
  ],
  "author": "Lushenggang <lushg-tcxg@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lushenggang/vue-search-highlight"
  },
  "license": "MIT",
  "main": "dist/vue-search-highlight.common.js",
  "unpkg": "dist/vue-search-highlight.min.js",
  "module": "dist/vue-search-highlight.esm.js",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name vue-search-highlight ./src/index.js",
    "prepublishOnly": "git push && npm run build-bundle"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
